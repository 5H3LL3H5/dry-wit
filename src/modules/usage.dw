# mod: usage

# txt: Prints the help message.
# fun: printHelp
# use: printHelp;
function printHelp() {

  if isFunctionPresent defineEnv "${SCRIPT_NAME}"; then
    defineEnv;
  fi
  usage $SCRIPT_NAME;
  printEnvironment;
  printExitValues;
}

# txt: Prints the usage message.
# fun: printUsage
# use: printUsage;
function printUsage() {
  printHelp >&2
}
