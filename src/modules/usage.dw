## Prints the help message.
## Example:
##   printHelp;
function printHelp() {

  if isFunctionPresent defineEnv "${SCRIPT_NAME}"; then
    defineEnv;
  fi
  usage $SCRIPT_NAME;
  printEnvironment;
  printExitValues;
}

## Prints the usage message.
## Example:
##   printUsage;
function printUsage() {
  printHelp >&2
}
